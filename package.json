{
  "name": "@hongbusi/contributors",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@7.12.0",
  "description": "Generate a list of contributors to your GitHub repository.",
  "author": "Hongbusi",
  "license": "MIT",
  "homepage": "https://github.com/Hongbusi/contributors#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hongbusi/contributors.git"
  },
  "bugs": "https://github.com/Hongbusi/contributors/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish --access public",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "consola": "^2.15.3",
    "md5": "^2.3.0",
    "octokit": "^2.0.7",
    "ora": "^6.1.2",
    "simple-git": "^3.14.1"
  },
  "devDependencies": {
    "@hongbusi/eslint-config": "^0.3.4",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.7.18",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "esno": "^0.16.3",
    "pnpm": "^7.12.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "vite": "^3.1.2",
    "vitest": "^0.23.4"
  }
}
